{
  "name": "Brain Bytes AI Assistant",
  "description": "A modern React application with Gmail OAuth integration and AI-powered features",
  "version": "1.0.0",
  "author": "Brain Bytes AI",
  "homepage": "https://preview--brain-bytes-ai-assistant.lovable.app",
  "deployment": {
    "type": "static",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "environmentVariables": {
      "optional": [
        "VITE_OAUTH_SERVER_URL",
        "VITE_SUPABASE_URL",
        "VITE_SUPABASE_ANON_KEY"
      ]
    }
  },
  "features": [
    "gmail-integration",
    "google-calendar",
    "ai-powered",
    "oauth-authentication",
    "productivity-tools"
  ]
}